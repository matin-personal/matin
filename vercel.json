{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/en/", "destination": "/en/index.html" },
    { "source": "/en", "destination": "/en/index.html" },
    { "source": "/contact/", "destination": "/contact/index.html" },
    { "source": "/contact", "destination": "/contact/index.html" }
  ],
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "en/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "contact/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "404.html",
      "use": "@vercel/static"
    }
  ]
}
